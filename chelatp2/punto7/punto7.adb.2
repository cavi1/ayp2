with Ada.Integer_Text_Io, Pila;
use Ada.Integer_Text_Io;

procedure Punto7 is
   
   package Pilaint is new Pila (Integer);
   subtype Pilaint3 is Tpila(3);
   
   Tvec is array (1..5) of Pilaint3;
   

   procedure Crearvec (Vec:out Tvec) is
      Pilaux: Pilaint3;
   begin
      for I in Tvec'First..Tvec'Last loop
         Crearpilas(Pilaaux);
         Vec(I):= Pilaaux;
         Limpiar(Pilaux);
      end loop;
   end Crearvec;


   procedure Crearpilas (Pila: out pilaint3) is
      X:Integer;
   begin
      for I in 1..3 loop
         Get(X);
         Meter(Pila,X);
      end loop;
   end Crearpilas;
   
